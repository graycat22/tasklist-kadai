<h1>ログイン</h1>
<%= form_with(url: login_path, scope: :session) do |f| %>

  <%= f.label :email, 'メールアドレス' %>
  <%= f.email_field :email %>
  
  <br>
  
  <%= f.label :password, 'パスワード' %>
  <%= f.password_field :password %>
  
  <br>
  
  <%= f.submit 'ログイン' %>
  
<% end %>

<p>もしくは <%= link_to 'サインアップ', signup_path %></p>